<% if doc.present? -%>
  <script type="text/javascript">
    window.brzvt = window.brzvt || {}
    window.brzvt.doc =  <%= doc.to_json.html_safe %>;
    <% if doc.registered_address.present? -%>
      brzvt.doc.registered_address = <%= doc.registered_address.to_json.html_safe %>;
    <% end -%>
    <% if doc.current_address.present? -%>
      brzvt.doc.current_address = <%= doc.current_address.to_json.html_safe %>;
    <% end -%>
    <% if doc.name.present? -%>
      brzvt.doc.name = <%= doc.name.to_json.html_safe %>;
    <% end -%>
    <% if doc.errors.present? -%>
      window.brzvt.errors = <%= doc.errors.to_json.html_safe %>;
    <% end -%>
  </script>
<% end %>