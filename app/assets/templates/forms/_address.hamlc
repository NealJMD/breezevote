- data_input = JST['helpers/data_input']

!= data_input(field: "#{@field}.street_address", ctx: @ctx)
!= data_input(field: "#{@field}.apartment", ctx: @ctx)
!= data_input(field: "#{@field}.city", ctx: @ctx)
!= data_input(field: "#{@field}.zip", ctx: @ctx)
!= data_input(field: "#{@field}.state", ctx: @ctx, disabled: (@field == "registered_address"))
- unless @field == "registered_address"
  != data_input(field: "#{@field}.country", ctx: @ctx)